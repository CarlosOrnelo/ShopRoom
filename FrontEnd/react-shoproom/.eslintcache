  return (\r\n      <Router>\r\n        <Switch>\r\n          <Route path='/categories/:id' component={Categories} />\r\n          <Route path='/product/:id' component={Product} />\r\n          {localStorage.getItem('x-auth-user') ? null : <Route path='/login' component={Login} />}\r\n          {!localStorage.getItem('x-auth-user') ? null : <Route path='/checkout' component={Checkout} />}\r\n          {!localStorage.getItem('x-auth-user') ? null : <Route path='/orders' component={Orders} />}\r\n          {!localStorage.getItem('x-auth-user') ? null : <Route path='/logout' component={Logout} />}\r\n          <Route path='/cart' component={Cart} />\r\n          <Route path='/products' component={Products} />\r\n          <Route path='/home' component={Home} />\r\n          <Redirect from='/' to='/home' component={Home} />\r\n        </Switch>\r\n      </Router>\r\n    );\r\n  }\r\n}\r\n\r\nexport default App;\r\n","D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\reportWebVitals.js",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\routes\\product.jsx",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\routes\\categories.jsx",["131"],"import React, { Component, Fragment } from 'react';\r\nimport Navbar from '../components/navbar';\r\nimport Banner from '../components/banner';\r\nimport Bottom from '../components/bottom';\r\nimport { getCategorie } from '../services/categoriesService';\r\nimport { ShowProducts } from '../components/showProducts';\r\n\r\nclass Categories extends Component {\r\n\r\n    state = { products: []\r\n    }\r\n\r\n    async componentDidMount() {\r\n        const products = await getCategorie(this.props.match.params.id);\r\n        this.setState({ products })\r\n    }\r\n\r\n    render() {\r\n        \r\n        return (\r\n            <React.Fragment>\r\n                <Navbar />\r\n                <Banner />\r\n                <div className='categories-container'>\r\n                    {this.state.products[0] ? ShowProducts(this.state.products) : <div><p>There are no products in this category!</p></div>}\r\n                </div>\r\n                <Bottom />\r\n            </React.Fragment>\r\n            );\r\n    }\r\n}\r\n\r\nexport default Categories;","D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\routes\\products.jsx",["132"],"import React, { Component, Fragment } from 'react';\r\nimport Navbar from '../components/navbar';\r\nimport Banner from '../components/banner';\r\nimport Bottom from '../components/bottom';\r\nimport { getProducts } from '../services/productsService';\r\nimport { ShowProducts } from '../components/showProducts';\r\nimport Pagination from '../components/pagination';\r\nimport SearchBar from '../components/searchBar';\r\n\r\nclass Products extends Component {\r\n    \r\n    state = {\r\n        products: [],\r\n        page: 1,\r\n        pageSize: 16,\r\n        searchQuery: ''\r\n      }\r\n      \r\n    async componentDidMount() {\r\n        let products = await getProducts();\r\n        this.setState({ products });\r\n    }\r\n\r\n    handleChange = page => \r\n        this.setState({ page })\r\n    \r\n    handleSearch = query => {\r\n        this.setState({ searchQuery: query });\r\n    };\r\n\r\n    \r\n    render() { \r\n        \r\n        let products = this.state.products.filter(product => product.name.toLowerCase().match(this.state.searchQuery.toLowerCase())) \r\n        \r\n        return (\r\n            <React.Fragment>\r\n                <SearchBar\r\n                    query={this.state.searchQuery}\r\n                    handleSearch={this.handleSearch}\r\n                />\r\n                <Navbar />\r\n                <Banner />\r\n                <p></p>\r\n                {ShowProducts(products, null, this.state.page, this.state.pageSize)}\r\n                <Pagination \r\n                    items={this.state.products.length}\r\n                    pageSize={this.state.pageSize}\r\n                    currentPage={this.state.page}\r\n                    handleChange={this.handleChange}\r\n                />\r\n                <Bottom />\r\n            </React.Fragment>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Products;\r\n","D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\routes\\home.jsx",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\services\\productsService.js",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\services\\categoriesService.js",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\components\\navbar.jsx",["133","134","135"],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\services\\httpService.js",["136"],"import axios from 'axios';\r\n\r\naxios.interceptors.response.use(null, error => {\r\n\r\n    const expectedError = error.response && error.response.status >= 400 && error.response.status < 500\r\n   \r\n    if (!expectedError) {\r\n        console.log(error);\r\n    }\r\n\r\n    return Promise.reject(error);\r\n});\r\n\r\nexport default {\r\n    get: axios.get,\r\n    post: axios.post,\r\n    put: axios.put,\r\n    delete: axios.delete\r\n}","D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\services\\changePageService.js",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\services\\productService.js",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\services\\cartService.js",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\routes\\cart.jsx",[],"D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\components\\addToCart.jsx",["137"],"import { toast, ToastContainer } from 'react-toastify';\r\n\r\nfunction addToCart (quantity, cartProducts, product) {\r\n      \r\n    let productsCart = cartProducts;\r\n\r\n    if(!quantity) {\r\n        quantity = 1\r\n    };\r\n    \r\n    if(!productsCart)\r\n        productsCart = [];\r\n    \r\n    let newProduct = productsCart.find(element => {\r\n        return element._id === product._id\r\n    });\r\n\r\n    if(!newProduct) {\r\n        newProduct = product;\r\n        newProduct.quantity = quantity;\r\n        productsCart.push(newProduct);\r\n        toast.success(`${product.name} added to your cart!`)\r\n    }\r\n    else {\r\n        productsCart.find((element, range) => {\r\n            return element._id === product._id && parseInt(element.quantity) + parseInt(quantity) <= 5 ? \r\n            (productsCart[range].quantity = parseInt(productsCart[range].quantity) + parseInt(quantity), toast.success(`${element.name} added to your cart!`)) : \r\n            toast.error('Maximum units on cart is 5!');\r\n        })\r\n    }\r\n\r\n    localStorage.setItem('shoproom-cart', JSON.stringify(productsCart))\r\n\r\n};\r\n\r\nexport default addToCart","D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\components\\showProducts.jsx",["138"],"import React from 'react';\r\nimport { changePage } from '../services/changePageService';\r\nimport { paginate } from '../utils/paginate';\r\nimport addToCart from './addToCart';\r\n\r\nexport function ShowProducts(products, remove, pageNumber, pageSize, tagName) {\r\n   \r\n    if(!pageNumber && !pageSize) {\r\n        pageNumber = 1;\r\n        pageSize = 99;\r\n    };\r\n\r\n    products = paginate(products, pageNumber, pageSize);\r\n    const routes = ['/home', '/cart', '/checkout'].find(element => element === window.location.pathname);\r\n    \r\n    return (\r\n\r\n        <div className={`products-${window.location.pathname.substring(1).split('/')[0]}`}>\r\n            {products.map((element, range) => \r\n                <div id='products-id' key={element._id || 'none'}>\r\n                    {remove ? <button className='remove-button' onClick={() => remove(element._id)}><i>Remove from cart</i></button> : null}\r\n                        <li className='product-details'>\r\n                            <img className='product-image' src={element.image} onClick={() => changePage(`/product/${element._id}`)}/>\r\n                            <h2>{element.name}</h2>\r\n                            <p>R${element.price}</p>\r\n                            <p className={tagName}>{element.quantity}</p>\r\n\r\n                            {!routes ? (\r\n                                <div className='insert-cart'>\r\n                                    <input className='input-quantity-cart' type=\"number\"\r\n                                        min=\"1\"\r\n                                        max={element.stock > 5 ? 5 : element.stock}\r\n                                        defaultValue='1'\r\n                                        onChange={e => element.quantity = e.target.value}\r\n                                    />\r\n                                    <button\r\n                                        className='addToCart'\r\n                                        disabled={element.quantity}\r\n                                        onClick={() => addToCart(element.quantity, JSON.parse(localStorage.getItem('shoproom-cart')), element)}\r\n                                    >\r\n                                        Add to cart\r\n                                    </button>\r\n                                </div>\r\n                            )\r\n                            :\r\n                                null\r\n                            }\r\n                        </li>\r\n                </div>\r\n            )}\r\n        </div>\r\n    );\r\n}\r\n","D:\\Arquivos\\Code\\ShopRoom\\frontend\\react-shoproom\\src\\components\\pagination.jsx",["139","140","141"],"import React from 'react';\r\nconst _ = require('lodash');\r\n\r\nconst Pagination = ({ items, pageSize, currentPage, handleChange }) => {\r\n\r\n    const pagesCount = Math.ceil(items/pageSize);\r\n    const pages = _.range(1, pagesCount + 1);\r\n\r\n    return (\r\n        <div className='pages'>\r\n            <ul className=\"pagination\">\